<?xml version="1.0" encoding="UTF-8"?>
<i:view xmlns:i="http://www.appflower.com/schema/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" type="layout" xsi:schemaLocation="http://www.appflower.com/schema/appflower.xsd">
    <i:title><![CDATA[dashboard]]></i:title>
    <i:area layout="2" type="content">
        <i:tab title="afCRM">
            <i:component column="0" module="afCrmAccounts" name="listAccounts"/>
            <i:component column="1" module="afCrmContacts" name="listContacts"/>
            <i:component column="1" module="afCrmDeals" name="listDeals"/>
            <i:component column="0" module="afCrmActivity" name="listActivities"/>
            <i:component column="0" module="afCrmStatus" name="listStatuses"/>
        </i:tab>
        <i:tab title="Master Detail" layout="1">
            <i:component column="0" name="listmd" module="product" bindForm="0"></i:component>
            <i:component column="0" name="addmd" module="product"></i:component>
        </i:tab>
        <i:tab title="Grid with row colors" layout="1">
            <i:component column="0" name="colorList" module="color"></i:component>
        </i:tab>
    </i:area>
</i:view>
